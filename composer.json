{
    "name": "sethsandaru/laravel-package-skeleton",
    "type": "library",
    "description": "Laravel Package Development Skeleton - Starter",
    "keywords": [
        "laravel",
        "laravel package development skeleton",
        "starter project for laravel package development"
    ],
    "authors": [
        {
            "name": "Phat Tran (Seth Phat)",
            "email": "phattranminh96@gmail.com",
            "homepage": "https://sethphat.com",
            "role": "Sr. Full-Stack Engineer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "^5.5"
    },
    "extra": {
        "laravel": {
             "providers": [
				"SethPhat\\:namespace\\ServiceProvider"
			],
            "alias": {
                ":package": "\\SethPhat\\:namespace\\Facade"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "SethPhat\\:namespace\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SethPhat\\:namespace\\Test\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
